<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_5_0.xsd" version="5.0">
  <servlet>
    <description></description>
    <display-name>DeconnectionServlet</display-name>
    <servlet-name>DeconnectionServlet</servlet-name>
    <servlet-class>org.projetEncheres.Servlets.DeconnectionServlet</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>ConnectionServlet</display-name>
    <servlet-name>ConnectionServlet</servlet-name>
    <servlet-class>org.projetEncheres.Servlets.ConnectionServlet</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>ConsulterEncheresHorsLigneServlet</display-name>
    <servlet-name>ConsulterEncheresHorsLigneServlet</servlet-name>
    <servlet-class>org.projetEncheres.Servlets.ConsulterEncheresHorsLigne</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>AccueilServlet</display-name>
