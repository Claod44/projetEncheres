    <servlet-class>org.projetEncheres.Servlets.AccueilServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeconnectionServlet</servlet-name>
    <url-pattern>/DeconnectionServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ConnectionServlet</servlet-name>
    <url-pattern>/Connection</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ConsulterEncheresHorsLigneServlet</servlet-name>
